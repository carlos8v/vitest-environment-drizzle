{
  "name": "vitest-environment-drizzle",
  "version": "0.2.2",
  "description": "Vitest testing environment for drizzle push scripts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig-build.json --module esnext --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig-build.json --module commonjs --outDir dist/cjs"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "keywords": [
    "drizzle",
    "vitest",
    "test"
  ],
  "author": "Carlos Souza <carlos.pessoal@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos8v/vitest-environment-drizzle.git"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "dotenv": "^16.0.3",
    "mysql2": "^3.6.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  },
  "peerDependencies": {
    "dotenv": "^16.0.3",
    "drizzle-kit": "^0.31.8",
    "mysql2": "^3.6.1",
    "vitest": ">=0.23.0"
  }
}
